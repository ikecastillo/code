<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="com.dt.jira.plugin.event.autotransition-to-readyfordevelopment-tests" name="autotransition-to-readyfordevelopment" plugins-version="2">
  <plugin-info>
    <description>This is the com.dt.jira.plugin.event:autotransition-to-readyfordevelopment plugin event for auto transitioned from indefination to readyfordevelopment when ticket is moved from backlog to any sprint on agile board</description>
    <version>1.0</version>
    <vendor name="Dealertrack" url="http://www.dealertrack.com/"/>
  </plugin-info>
  <!-- from our base plugin -->
  <component-import key="myComponent" interface="com.dt.jira.plugin.event.MyPluginComponent"/>
  <!-- from the product container -->
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>
  <component key="my-component-wired-test" class="it.com.dt.jira.plugin.event.MyComponentWiredTest" public="true">
    <interface>it.com.dt.jira.plugin.event.MyComponentWiredTest</interface>
    <service-properties>
      <entry key="inProductTest" value="true"/>
    </service-properties>
  </component>
</atlassian-plugin>
