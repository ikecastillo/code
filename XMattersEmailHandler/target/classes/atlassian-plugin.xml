        technical conference bridge numbers found at the end of the mail.</description><version>1.3</version><vendor name="Dealertrack" url="https://us.dealertrack.com"/><param name="plugin-icon">images/pluginIcon.png</param><param name="plugin-logo">images/pluginLogo.png</param></plugin-info><resource type="i18n" name="i18n" location="XMattersEmailHandler"/><web-resource key="XMattersEmailHandler-resources" name="XMattersEmailHandler Web Resources"><dependency>com.atlassian.auiplugin:ajs</dependency><resource type="download" name="XMattersEmailHandler.css" location="/css/XMattersEmailHandler.css"/><resource type="download" name="XMattersEmailHandler.js" location="/js/XMattersEmailHandler.js"/><resource type="download" name="images/" location="/images"/><context>XMattersEmailHandler</context></web-resource><message-handler i18n-name-key="xMattersEHandler.name" key="xMattersEHandler.name" class="com.dt.plugin.xmattersehandler.emailhandler.XMattersEmailHandler" add-edit-url="/secure/admin/EditHandlerDetailsUsingParams!default.jspa" weight="0"/><component key="email-parser" name="email-parser" class="com.dt.plugin.xmattersehandler.impl.EmailParserImpl" public="true"><interface>com.dt.plugin.xmattersehandler.api.EmailParser</interface></component></atlassian-plugin>