<html>
   <head>
      <meta name="decorator" content="atl.admin" />
      <meta name="projectKey" content="$projectKey"/>
      <meta name="application-base-url" content="$applicationProperties.getBaseUrl()"> 
      <meta name="admin.active.section" content="atl.jira.proj.config"/>       
       $webResourceManager.requireResource("com.dt.jira.servicedesk.customize.DT-ServiceDesk-Customization:DT-ServiceDesk-Customization-resources") 
       $webResourceManager.requireResource("com.atlassian.auiplugin:aui-select2") 
     <title>Edit Support Automation</title>
     
     
</head>
<body>
<div class="content-container">
<form  method="get"   class="aui" >
    
   						
                           
                           <table class="automationcss">
  							<tr>
                              <td>
                              <label for="projectList" style="float:left;">Status</label>
  								<div class="subTask_col2">
   							     <select id="status" name="status">                                                             
                                    <option value="None">---select---</option>                                    
                                    #foreach ($option in $statusList)                                    
	                                     #if ($selectedStatus && ($selectedStatus  == $option.getName()))
	                   							<option value="$option.getName()" selected="selected">$option.getName()</option>
	              						 #else
	                  							<option value="$option.getName()">$option.getName()</option>
	            						 #end
                                     #end 
                                </select>                                                             
                             </div>
                           </td>
                         
                              
                           
                              <td>
                              <label for="approval" style="float:left;">Manager Approval (Y/N)</label>
  								<div class="subTask_col3">
   							     <select id="approval" name="approval">                                                             
                                    <option value="None">---select---</option>
                                    <option value="No">No</option>                                  
                               </select>                                                             
                              </div>
                            </td>
                            
                            <td >
                            <label for="projectList" style="float:left;">Assignee</label>
                              	<div>
    							<fieldset rel="multiUserPicker" class="hidden user-picker-params">
   								 <input title="formName" type="hidden" value="jiraform" />
    							<input  title="fieldName" type="hidden" value="multiUserPicker" />
   								 <input title="multiSelect" type="hidden" value="true" />
    							<input title="userPickerEnabled" type="hidden" value="true" /></fieldset>
    							<div class="ajax_autocomplete" id="multiUserPicker_container">
        						 <textarea class="textarea long-field" cols="40" id="multiUserPicker" name="multiUserPicker" rows="3" style="width:250px;"></textarea>
       						      <a class="popup-trigger" href="#"><span class="aui-icon icon-userpicker" title="Select a user(s)"></span></a>
        						<div id="multiUserPicker_results" class="ajax_results"></div>
        						</div>
							</div>
						   </td>
                            
                            <td style="width:70px;">
                             <input type="button" name="save" id="btnp_Save" value="Save"  class="button spaced" />
                              </td>
                         
                     </tr>
                          
                    </table>
                   
                    
                    </div>
                     
                     
                      
                       
   </form>   
</div>
</body>
</html>